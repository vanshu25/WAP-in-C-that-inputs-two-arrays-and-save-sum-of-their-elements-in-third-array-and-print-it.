#include<stdio.h>
void main()
{
int i, arr1[100], arr2[100], arr3[100];
printf("enter the elements of array1:\n");
for(i=0;i<5;i++)
{
    scanf("%d",&arr1[i]);

}
printf("enter the elements of array2:\n");
for(i=0;i<5;i++)
{
    scanf("%d",&arr2[i]);

}
for(i=0;i<5;i++)
{
    arr3[i] = arr2[i] + arr1[i];
     printf("Resulted array:%d\n",rr3[i]);

}

}
